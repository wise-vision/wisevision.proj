{
  "mode": "router",
  "plugins": {
    "ros2dds": {
      "nodename": "zenoh_bridge",
      "ros_localhost_only": false
    },
    "storage_manager": {
      "volumes": {
        "influxdb": {
          "url": "http://localhost:8086"
        }
      },
      "storages": {
        "demo": {
          "key_expr": "sensor_publisher_e5_board_temp/**",
          "volume": {
            "id": "influxdb",
            "db": "zenoh_example_1",
            "create_db": true,
            "on_closure": "do_nothing",
            "private": {
              "encoding": "base64"
            }
          }
        },
        "devices": {
          "key_expr": "devices_data/**",
          "volume": {
            "id": "influxdb",
            "db": "zenoh_devices_data",
            "create_db": true,
            "on_closure": "do_nothing"
          }
        },
        "demo_test": {
          "key_expr": "notifications/**",
          "volume": {
            "id": "influxdb",
            "db": "zenoh_example_2",
            "create_db": true,
            "on_closure": "do_nothing",
            "private": {
              "encoding": "base64"
            }
          }
        }
      }
    },
    "rest": {
      "http_port": 8000
    }
  },
  "listen": {
    "endpoints": [
      "tcp/0.0.0.0:7447"
    ]
  }
}